@model List<ServiciosTI.BL.Producto>
@{
    ViewBag.Title = "Index";
}

<h2>Productos</h2>

 <table class="table-bordered table-hover">
    <thead>
        <tr>
            <th>
                codigo
            </th>
            <th>
                Descripcion
            </th>
            <th>
                Precio
            </th>
        </tr>
    </thead>
  @foreach (var producto in Model)

  {
    <tr>
        <td>@producto.Id</td>
        <td>@producto.Descripcion</td>
        <td>@producto.Precio</td>
    </tr>

    }
  
     
 </table>